{
  "name": "thorbis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "pnpm clean && next dev",
    "dev:strict": "concurrently -k -s first -n next,tsc,lint -c cyan,green,magenta \"next dev --no-lint\" \"pnpm typecheck:watch\" \"pnpm lint:eslint:watch\"",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=4096' next build --webpack",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=4096' SKIP_ENV_VALIDATION=true next build --webpack",
    "verify": "pnpm typecheck && pnpm lint:eslint",
    "sitemap": "next-sitemap --config next-sitemap.config.mjs",
    "seo:validate": "tsx scripts/testing/validate-structured-data.ts",
    "start": "next start",
    "clean": "rimraf .next node_modules/.cache",
    "lint": "pnpm dlx ultracite check",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0 .",
    "lint:eslint:watch": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0 --watch .",
    "lint:fix": "pnpm dlx ultracite fix",
    "typecheck": "tsc --noEmit --pretty",
    "typecheck:watch": "tsc --noEmit --pretty --watch",
    "analyze": "pnpm run analyze:knip && pnpm run analyze:deps && pnpm run analyze:circular",
    "analyze:bundle": "ANALYZE=true pnpm build",
    "analyze:knip": "knip",
    "analyze:knip:production": "knip --production",
    "analyze:ts-prune": "ts-prune",
    "analyze:deps": "depcheck --ignores=\"postcss,@tailwindcss/postcss,@types/google.maps,rimraf,critters\"",
    "analyze:circular": "madge --circular --extensions ts,tsx ./src",
    "analyze:graph": "madge --image graph.svg --extensions ts,tsx ./src",
    "setup:default-user": "tsx scripts/setup/setup-default-user-with-payment.ts",
    "generate:test-data": "tsx scripts/generate-invoices-payments.ts",
    "prepare": "husky install"
  },
