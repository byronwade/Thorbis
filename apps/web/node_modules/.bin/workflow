#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules/workflow/bin/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules/workflow/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules/workflow/bin/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules/workflow/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/workflow@4.0.1-beta.13_@aws-sdk+client-sts@3.932.0_@opentelemetry+api@1.9.0_@swc+helpers@0.5._l5d45cl4kvb6hky2wjonsr2oky/node_modules:/Users/byronwade/Stratos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../workflow/bin/run.js" "$@"
else
  exec node  "$basedir/../workflow/bin/run.js" "$@"
fi
