{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}!",
    "src/app/**/layout.tsx",
    "src/app/**/page.tsx"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/test/**",
    "**/tests/**",
    "**/__tests__/**",
    "node_modules/**",
    "src/components/ui/**",
    "src/components/chat/**",
    "src/lib/ai/examples.tsx",
    "src/lib/store/examples.tsx",
    "src/lib/supabase/**",
    "src/hooks/use-mobile.ts",
    "src/components/layout/index.ts",
    "src/actions/**/*.ts",
    "src/types/**/*.ts",
    "src/components/lazy/**",
    "src/components/offline/**",
    "src/components/schedule/**",
    "src/components/tv-leaderboard/**",
    "src/lib/ai/**",
    "src/lib/analytics/**",
    "src/lib/services/**",
    "src/lib/seo/**",
    "src/lib/email/**",
    "src/lib/telnyx/**",
    "src/lib/storage/**",
    "src/lib/offline/**",
    "src/lib/payments/**",
    "src/lib/search/**",
    "src/lib/window/**",
    "src/lib/kb/**",
    "src/lib/content/**",
    "src/lib/data/**",
    "src/lib/db/**",
    "src/lib/auth/**",
    "src/lib/company/**",
    "src/lib/formatters/**",
    "src/lib/hooks/**",
    "src/lib/layout/**",
    "src/lib/maps/**",
    "src/lib/marketing/**",
    "src/lib/notifications/**",
    "src/lib/settings/**",
    "src/lib/sidebar/**",
    "src/lib/utils/**",
    "src/lib/validations/**",
    "src/lib/stores/**",
    "src/hooks/**"
  ],
  "ignoreDependencies": [
    "@types/*",
    "ultracite",
    "knip",
    "ts-prune",
    "depcheck",
    "madge",
    "@radix-ui/*",
    "class-variance-authority",
    "clsx",
    "tailwind-merge",
    "lucide-react",
    "cmdk",
    "vaul",
    "sonner",
    "input-otp",
    "react-day-picker",
    "react-hook-form",
    "@hookform/resolvers",
    "zod",
    "zod-to-json-schema",
    "framer-motion",
    "embla-carousel-react",
    "react-resizable-panels",
    "recharts",
    "date-fns",
    "nanoid",
    "classnames",
    "fast-deep-equal",
    "usehooks-ts",
    "use-stick-to-bottom",
    "swr",
    "shiki",
    "react-syntax-highlighter",
    "streamdown",
    "resumable-stream",
    "papaparse",
    "diff-match-patch",
    "orderedmap",
    "codemirror",
    "@codemirror/*",
    "prosemirror-*",
    "react-data-grid",
    "next-themes",
    "@vercel/blob",
    "@supabase/*",
    "bcrypt-ts",
    "ai",
    "@ai-sdk/*",
    "husky",
    "lint-staged",
    "shadcn",
    "workbox-window",
    "next-pwa",
    "@next/eslint-plugin-next",
    "@stripe/react-stripe-js",
    "@stripe/stripe-js",
    "@tiptap/core",
    "@tiptap/extension-code-block",
    "@tiptap/extension-code-block-lowlight",
    "@tiptap/extension-image",
    "@tiptap/extension-link",
    "@tiptap/extension-mention",
    "@tiptap/extension-placeholder",
    "@tiptap/extension-table",
    "@tiptap/extension-table-cell",
    "@tiptap/extension-table-header",
    "@tiptap/extension-table-row",
    "@tiptap/extension-text-style",
    "@tiptap/pm",
    "@tiptap/react",
    "@tiptap/starter-kit",
    "@tiptap/suggestion",
    "critters",
    "lodash.throttle",
    "novel",
    "rehype",
    "remark",
    "remark-html",
    "three",
    "uuid",
    "@biomejs/biome",
    "@jest/globals",
    "eslint-config-next",
    "eslint-plugin-react-hooks",
    "glob"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": ["shadcn", "next-pwa"],
  "ignoreWorkspaces": [],
  "next": {
    "entry": [
      "src/app/**/*.tsx",
      "src/app/**/*.ts",
      "!src/app/**/*.test.{ts,tsx}",
      "!src/app/**/*.spec.{ts,tsx}"
    ]
  }
}
