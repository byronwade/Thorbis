{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env*"],
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"],
			"env": [
				"NODE_ENV",
				"SUPABASE_SERVICE_ROLE_KEY",
				"SUPABASE_JWT_SECRET",
				"DATABASE_URL",
				"POSTGRES_URL",
				"POSTGRES_URL_NON_POOLING",
				"POSTGRES_PRISMA_URL",
				"POSTGRES_HOST",
				"POSTGRES_USER",
				"POSTGRES_PASSWORD",
				"POSTGRES_DATABASE",
				"STRIPE_SECRET_KEY",
				"STRIPE_WEBHOOK_SECRET",
				"STRIPE_PRICE_ID_BASE_PLAN",
				"STRIPE_PRICE_ID_ADDITIONAL_ORG",
				"STRIPE_PRICE_ID_TEAM_MEMBERS",
				"STRIPE_PRICE_ID_STORAGE",
				"STRIPE_PRICE_ID_API_CALLS",
				"STRIPE_PRICE_ID_WORKFLOWS",
				"STRIPE_PRICE_ID_PAYMENTS",
				"STRIPE_PRICE_ID_PHONE_MINUTES",
				"STRIPE_PRICE_ID_VIDEO_MINUTES",
				"STRIPE_PRICE_ID_SMS",
				"STRIPE_PRICE_ID_EMAILS",
				"STRIPE_PRICE_ID_ESTIMATES",
				"STRIPE_PRICE_ID_INVOICES",
				"TELNYX_API_KEY",
				"TELNYX_PUBLIC_KEY",
				"TELNYX_WEBHOOK_SECRET",
				"TELNYX_SKIP_SIGNATURE_VERIFICATION",
				"RESEND_API_KEY",
				"RESEND_FROM_EMAIL",
				"RESEND_FROM_NAME",
				"PLAID_CLIENT_ID",
				"PLAID_SECRET",
				"PLAID_ENV",
				"AI_GATEWAY_URL",
				"AI_GATEWAY_TOKEN",
				"AI_PROVIDER",
				"AI_MODEL",
				"OPENAI_API_KEY",
				"ANTHROPIC_API_KEY",
				"GOOGLE_GENERATIVE_AI_API_KEY",
				"GOOGLE_PLACES_API_KEY",
				"HUNTER_API_KEY",
				"RENTCAST_API_KEY",
				"CRON_SECRET",
				"INVITATION_SECRET",
				"RESEND_WEBHOOK_SECRET",
				"ENABLE_SPAM_FILTER",
				"AI_SPAM_CONFIDENCE_THRESHOLD",
				"SPAM_FILTER_THRESHOLD",
				"SPAM_FILTER_UNCERTAIN_THRESHOLD",
				"TWILIO_ACCOUNT_SID",
				"TWILIO_AUTH_TOKEN",
				"TWILIO_PHONE_NUMBER",
				"TWILIO_MESSAGING_SERVICE_SID",
				"TWILIO_WEBHOOK_URL",
				"PLATFORM_EMAIL_DOMAIN",
				"GOOGLE_SEARCH_ENGINE_ID",
				"GOOGLE_CLIENT_ID",
				"GOOGLE_CLIENT_SECRET",
				"TOKEN_ENCRYPTION_KEY",
				"REDIS_ENABLE_WEBHOOK_DEDUP",
				"REDIS_ENABLE_RATE_LIMITING",
				"REDIS_ENABLE_SESSION_CACHE",
				"SKIP_ENV_VALIDATION"
			]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"clean": {
			"cache": false
		}
	}
}
